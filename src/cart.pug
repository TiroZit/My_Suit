extends ../assets/pug/_base
block vars
  - var lang = 'ru'
  - var title = 'My Suit | Корзина'
  - var description = ''
  - var keywords = ''
  //- Open Graph:
  - var type = 'website'
  - var imageOG = ''
  //- Preload fonts
  - var fonts = ['Manrope-Medium', 'Manrope-Regular', 'Manrope-Light' ]

include pug/_breadcrumbs
block content
  .wrapper
    include pug/_header
    main.page.page_order-successfully
      +breadcrumbs([
        {
          name: "Главная",
          path: "home.html"
        },
        {
          name: "Корзина",
          path: "cart.html"
        }
      ])
    include pug/_footer